n =int(input("Ввод первого числа"))
m = int(input("Ввод второго числа"))
print("Среднее арифметическое",(n+m)/2)
print("Среднее геометрическое",(n-m)^(1/2))
x = int(input("Введите число x:"))
a = int(input("Введите число a:"))
if x<1:
    print("Ответ:",(x**2)+(a**2))
elif x>1: 
    print("Ответ:",(x**2)+(a**2))
